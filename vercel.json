{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/ai-proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_GOOGLE_MAPS_API_KEY": "@react_app_google_maps_api_key",
    "REACT_APP_AI_API_KEY": "@react_app_ai_api_key",
    "REACT_APP_AI_API_URL": "@react_app_ai_api_url",
    "REACT_APP_AI_MODEL": "@react_app_ai_model"
  }
}
