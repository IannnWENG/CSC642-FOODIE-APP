# Google Maps API 配置
# 請前往 https://console.cloud.google.com/ 獲取 API 金鑰
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# AI 服務配置 (可選)
# 如果您要使用 AI 聊天功能，請設定以下變數
REACT_APP_AI_API_KEY=your_ai_api_key_here
REACT_APP_AI_API_URL=https://your-ai-service-endpoint.com/v1/chat/completions
REACT_APP_AI_MODEL=gpt-3.5-turbo

# 其他配置
REACT_APP_APP_NAME=Restaurant Recommender
REACT_APP_APP_VERSION=1.0.0
